<div class="mt-5">
  <%= form_tag(users_path, :method => 'get' ) do %>
    <%= text_field_tag :search, 'find user here'%>
    <%= submit_tag 'Search', :name => nil %>
  <% end %>
</div>
<%= render 'users/users', users: @users, title: @title %>
<% if @need_back == "need" %>
  <%= link_to 'Back to Users List', users_path, class: 'font-weight-bold' %>
<% end %>
